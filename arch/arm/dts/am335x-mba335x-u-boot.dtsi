#include "am335x-tqma335x-u-boot.dtsi"

/ {
	config {
		u-boot,mmc-env-offset = <0x260000>;
		u-boot,mmc-env-offset-redundant = <0x280000>;
		u-boot,spl-payload-offset = <0x20000>;
	};

	sound {
		status = "disabled";
	};
};

&dcan0 {
	status = "disabled";
};

&dcan1 {
	status = "disabled";
};

&mac {
	/delete-property/ pinctrl-names;
	/delete-property/pinctrl-0;
	/delete-property/ dual_emac;
};

&mcasp0 {
	status = "disabled";
};

&mmc1_pins {
	u-boot,dm-pre-reloc;
	u-boot,dm-spl;
};

&mmc1 {
	u-boot,dm-pre-reloc;
	u-boot,dm-spl;
};

&tlv320aic32x4 {
	status = "disabled";
};

&uart4_pins {
	u-boot,dm-pre-reloc;
	u-boot,dm-spl;
};

&mmc2 {
	u-boot,dm-pre-reloc;
	u-boot,dm-spl;
};
